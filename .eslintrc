{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": "off",
    "func-names": "off",
    "prefer-arrow-callback": "off",
    "callback-return": "off",
    "no-useless-escape": "off",
    "prefer-spread": "off"
  }
}
