{
  "name": "mycolorconverter",
  "version": "1.1.2",
  "description": "The application can be used to convert either HEX to RGB or RGB to HEX",
  "private": false,
  "scripts": {
    "test": "vitest run --segfault-retry=3",
    "test-coverage": "vitest run --coverage",
    "test-in-browser": "vitest --ui",
    "start": "vite serve --port 5000",
    "build": "vite build --mode='production'",
    "preview": "vite --port 5050"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rahulbaran/myColorConverter.git"
  },
  "keywords": [
    "color-converter",
    "convert-color",
    "convert hex to rgb",
    "convert rgb to hex",
    "rgb to hex",
    "hex to rgb"
  ],
  "author": "Rahulbaran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rahulbaran/myColorConverter/issues"
  },
  "homepage": "https://github.com/Rahulbaran/myColorConverter#readme",
  "browserslist": {
    "production": [
      "> .25%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitest/coverage-c8": "^0.23.4",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  }
}
